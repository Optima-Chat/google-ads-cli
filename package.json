{
  "name": "@optima-chat/google-ads-cli",
  "version": "0.2.0",
  "description": "用自然语言管理 Google Ads 广告投放 - 专为 Claude Code 设计的对话式 CLI 工具",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "google-ads": "./dist/index.js"
  },
  "files": [
    "dist",
    ".claude",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsx src/index.ts",
    "start": "node dist/index.js",
    "prepublishOnly": "npm run build",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "google-ads",
    "google-ads-api",
    "cli",
    "advertising",
    "claude-code",
    "ai",
    "natural-language",
    "gaql"
  ],
  "author": "Optima Commerce Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Optima-Chat/google-ads-cli.git"
  },
  "homepage": "https://github.com/Optima-Chat/google-ads-cli",
  "bugs": {
    "url": "https://github.com/Optima-Chat/google-ads-cli/issues"
  },
  "engines": {
    "node": ">=18"
  },
  "dependencies": {
    "axios": "^1.6.5",
    "chalk": "^5.3.0",
    "cli-table3": "^0.6.5",
    "commander": "^12.0.0",
    "conf": "^12.0.0",
    "dotenv": "^17.2.3",
    "express": "^4.18.2",
    "google-ads-api": "^21.0.1",
    "inquirer": "^9.2.14",
    "open": "^10.0.3",
    "ora": "^8.0.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/inquirer": "^9.0.9",
    "@types/node": "^20.11.5",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "eslint": "^8.56.0",
    "prettier": "^3.2.4",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  }
}
